{
  "name": "@vegify/b12",
  "version": "0.0.5",
  "author": "John Carmack",
  "license": "ISC",
  "description": "B12 is a React component library built using SWC, TypeScript, and Rollup.",
  "source": "src/index.ts",
  "main": "dist/main.js",
  "module": "dist/module.js",
  "types": "dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "cleanup": "rm -rf dist node_modules storybook-static",
    "build": "rollup -c",
    "watch": "rollup -cw",
    "storybook": "run-p watch:*",
    "build-storybook": "run-p build:*",
    "build:css": "npx tailwindcss -i ./src/index.css -o ./public/index.css",
    "build:storybook": "storybook build",
    "watch:css": "npx tailwindcss -i ./src/index.css -o ./public/index.css --watch",
    "watch:storybook": "storybook dev -p 6006",
    "chromatic": "chromatic --build-script-name build:storybook --project-token=CHROMATIC_PROJECT_TOKEN --exit-zero-on-chages"
  },
  "devDependencies": {
    "@headlessui/react": "^1.7.7",
    "@rollup/plugin-eslint": "^9.0.3",
    "@storybook/addon-a11y": "7.0.0-beta.31",
    "@storybook/addon-essentials": "^7.0.0-beta.31",
    "@storybook/addon-interactions": "^7.0.0-beta.31",
    "@storybook/addon-links": "^7.0.0-beta.31",
    "@storybook/blocks": "^7.0.0-beta.31",
    "@storybook/builder-webpack5": "^7.0.0-beta.31",
    "@storybook/cli": "7.0.0-beta.31",
    "@storybook/manager-webpack5": "^7.0.0-alpha.10",
    "@storybook/react": "^7.0.0-beta.31",
    "@storybook/react-webpack5": "^7.0.0-beta.31",
    "@storybook/testing-library": "^0.0.13",
    "@svgr/cli": "^6.5.1",
    "@svgr/rollup": "^6.5.1",
    "@swc/core": "^1.3.27",
    "@types/react": "^18.0.27",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "chromatic": "^6.14.0",
    "clsx": "^1.2.1",
    "eslint": "^8.32.0",
    "eslint-plugin-storybook": "^0.6.10",
    "eslint-plugin-tailwindcss": "^3.8.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "prettier": "^2.8.3",
    "prettier-plugin-tailwindcss": "^0.2.1",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^3.10.1",
    "rollup-plugin-dts": "^5.1.1",
    "rollup-plugin-swc3": "^0.8.0",
    "speed-measure-webpack-plugin": "^1.5.0",
    "storybook": "^7.0.0-beta.31",
    "storybook-addon-swc": "^1.1.9",
    "tailwind-merge": "^1.8.1",
    "tailwindcss": "^3.2.4",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4",
    "webpack": "^5.75.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johncarmack1984/b12.git"
  },
  "keywords": [
    "react",
    "component",
    "tailwind",
    "rollup",
    "typescript"
  ],
  "bugs": {
    "url": "https://github.com/johncarmack1984/b12/issues"
  },
  "homepage": "https://github.com/johncarmack1984/b12#readme"
